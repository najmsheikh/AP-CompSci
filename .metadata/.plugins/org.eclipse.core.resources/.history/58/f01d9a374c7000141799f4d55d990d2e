
public class Pascal {

	public static void main(String[] args) {
		int[][] numz = new int[10][10];
		numz[0][0] = 1;
		numz[1][0] = 1;
		numz[1][1] = 1;
		
		for(int r=2;r<numz.length;r++){
			for(int c=1;c<numz.length;c++){
				numz[r][0] = 1;
				numz[r][c] = numz[r-1][c-1]+numz[r-1][c];
				System.out.println(numz[r][c]);
			}
		}
	}
}
