
public class Matrice {

	public static void main(String[] args) {
		int[][] set1 ={{1,2,3},{4,5,6}};
		int[][] set2 ={{7,8},{9,10},{11,12}};
		int[][] finalset = new int[2][2];
		int product=0;
		
		for(int i=0;i<2;i++){
			for(int j=0;j<3;j++){
				int sum = 0;
				int prod = (set1[i][j] * set2[i][j]);
				sum += prod;
				System.out.println(sum + " " + prod);
			}
		}
	}
}
